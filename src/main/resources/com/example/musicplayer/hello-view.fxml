<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.Menu?>
<?import javafx.scene.control.MenuBar?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.control.Slider?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<AnchorPane prefHeight="343.0" prefWidth="819.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.musicplayer.controller.HomeController">
   <children>
      <MenuBar prefHeight="25.0" prefWidth="819.0">
        <menus>
          <Menu mnemonicParsing="false" text="File">
            <items>
              <MenuItem mnemonicParsing="false" text="Close" />
            </items>
          </Menu>
          <Menu mnemonicParsing="false" text="Edit">
            <items>
              <MenuItem mnemonicParsing="false" text="Delete" />
            </items>
          </Menu>
          <Menu mnemonicParsing="false" text="Help">
            <items>
              <MenuItem mnemonicParsing="false" text="About" />
            </items>
          </Menu>
        </menus>
      </MenuBar>
      <Slider fx:id="audioTimeSlider" layoutX="186.0" layoutY="182.0" prefHeight="25.0" prefWidth="308.0" />
      <Label fx:id="songNameLabel" alignment="CENTER" layoutX="278.0" layoutY="79.0" prefHeight="25.0" prefWidth="79.0" text="Song name" />
      <VBox layoutX="652.0" layoutY="52.0" prefHeight="200.0" prefWidth="127.0">
         <children>
            <ListView fx:id="fileListView" prefHeight="200.0" prefWidth="200.0" style="-fx-border-color: black; -fx-border-width: 1;">
               <opaqueInsets>
                  <Insets />
               </opaqueInsets>
            </ListView>
         </children>
      </VBox>
      <Button layoutX="61.0" layoutY="221.0" mnemonicParsing="false" onAction="#playPause" text="Play" />
      <Button layoutX="125.0" layoutY="221.0" mnemonicParsing="false" onAction="#reset" text="Reset" />
      <Button layoutX="186.0" layoutY="221.0" mnemonicParsing="false" onAction="#previousSong" text="Previous" />
      <Button layoutX="265.0" layoutY="221.0" mnemonicParsing="false" onAction="#nextSong" text="Next" />
      <VBox alignment="CENTER" layoutX="500.0" layoutY="130.0" prefHeight="130.0" prefWidth="79.0">
         <children>
            <Slider fx:id="speedSlider" max="200.0" orientation="VERTICAL" prefHeight="102.0" prefWidth="24.0" value="100.0" />
            <Label text="Speed" />
         </children>
      </VBox>
      <VBox alignment="CENTER" layoutX="569.0" layoutY="139.0" prefHeight="136.0" prefWidth="66.0">
         <children>
            <Slider fx:id="volumeSlider" blockIncrement="100.0" max="200.0" orientation="VERTICAL" prefHeight="85.0" prefWidth="24.0" value="100.0" />
            <Label prefHeight="25.0" prefWidth="54.0" text="Volume" />
         </children>
      </VBox>
   </children>
</AnchorPane>
